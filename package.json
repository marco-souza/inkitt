{
	"name": "inkitt",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prepare-repository": "npm i -g react-native-cli yarn;yarn install; react-native link",
		"emulator": "emulator @lg-screen -use-system-libs -scale 1.0",
		"install": "react-native run-android",
		"start": "react-native start --reset-cache",
		"android": "npm run install && npm run start",
		"clean": "watchman watch-del-all && npm cache clean && cd android && ./gradlew clean && cd ..",
		"test": "NODE_ENV=testing node_modules/.bin/jest --config jest.config.json"
	},
	"dependencies": {
		"axios": "^0.16.2",
		"react-native": "0.47.2",
		"react-native-htmlview": "^0.12.0",
		"react-native-loader": "^1.1.0"
	},
	"devDependencies": {
		"babel-jest": "20.0.3",
		"babel-preset-jest": "^20.0.3",
		"babel-preset-react-native": "3.0.2",
		"enzyme": "^2.9.1",
		"jest": "20.0.4",
		"jest-enzyme": "^3.8.1",
		"react": "^15.6.0",
		"react-addons-test-utils": "^15.6.0",
		"react-dom": "^15.6.1",
		"react-native-mock": "^0.3.1",
		"react-test-renderer": "15.6.1"
	}
}
